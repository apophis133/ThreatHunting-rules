rule CamHacker
{
    meta:
        description = "Detection patterns for the tool 'CamHacker' taken from the ThreatHunting-Keywords github project" 
        author = "@mthcht"
        reference = "https://github.com/mthcht/ThreatHunting-Keywords"
        tool = "CamHacker"
        rule_category = "offensive_tool_keyword"

    strings:
        // Description: Camera phishing tool. If anyone opens link generated by CamHacker and permits camera access his/her photo will be captured!
        // Reference: https://github.com/KasRoudra/CamHacker
        $string1 = /\scamhacker\s/ nocase ascii wide
        // Description: Camera phishing tool. If anyone opens link generated by CamHacker and permits camera access his/her photo will be captured!
        // Reference: https://github.com/KasRoudra/CamHacker
        $string2 = /\scamhacker\:\/CamHacker/ nocase ascii wide
        // Description: Camera phishing tool. If anyone opens link generated by CamHacker and permits camera access his/her photo will be captured!
        // Reference: https://github.com/KasRoudra/CamHacker
        $string3 = /\stermux\-chroot\s.{0,1000}\/cloudflared/ nocase ascii wide
        // Description: Camera phishing tool. If anyone opens link generated by CamHacker and permits camera access his/her photo will be captured!
        // Reference: https://github.com/KasRoudra/CamHacker
        $string4 = /\$Hc2\$w\$c\$rQW\$d\$s\$w\$b\$Hc2\$v\$xZp\$f\$w\$V9z\$rQW\$L\$U\$xZp/ nocase ascii wide
        // Description: Camera phishing tool. If anyone opens link generated by CamHacker and permits camera access his/her photo will be captured!
        // Reference: https://github.com/KasRoudra/CamHacker
        $string5 = /\$HOME\/\.tunneler/ nocase ascii wide
        // Description: Camera phishing tool. If anyone opens link generated by CamHacker and permits camera access his/her photo will be captured!
        // Reference: https://github.com/KasRoudra/CamHacker
        $string6 = /\$N0q\$x\$Hc2\$rQW/ nocase ascii wide
        // Description: Camera phishing tool. If anyone opens link generated by CamHacker and permits camera access his/her photo will be captured!
        // Reference: https://github.com/KasRoudra/CamHacker
        $string7 = /\$tunneler_dir\/loclx\.log/ nocase ascii wide
        // Description: Camera phishing tool. If anyone opens link generated by CamHacker and permits camera access his/her photo will be captured!
        // Reference: https://github.com/KasRoudra/CamHacker
        $string8 = /\.tunneler\/cf\.log/ nocase ascii wide
        // Description: Camera phishing tool. If anyone opens link generated by CamHacker and permits camera access his/her photo will be captured!
        // Reference: https://github.com/KasRoudra/CamHacker
